{
  "name": "@vibebase/react",
  "version": "0.1.0",
  "description": "React SDK for Vibebase - Personal BaaS on Cloudflare",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check src --write"
  },
  "keywords": [
    "vibebase",
    "react",
    "hooks",
    "baas",
    "backend-as-a-service",
    "cloudflare",
    "typescript"
  ],
  "author": "Vibebase Team",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibebase/vibebase.git",
    "directory": "packages/sdk-react"
  },
  "homepage": "https://github.com/vibebase/vibebase",
  "bugs": {
    "url": "https://github.com/vibebase/vibebase/issues"
  },
  "dependencies": {
    "@vibebase/sdk": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=16"
  }
}