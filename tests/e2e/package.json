{
  "name": "@vibebase/e2e-tests",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:clean": "npm run setup && npm run seed && npm run test",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "setup": "tsx --env-file .env.test team-task-app/setup.ts",
    "seed": "tsx --env-file .env.test team-task-app/seed.ts",
    "teardown": "tsx --env-file .env.test team-task-app/teardown.ts",
    "simple-test": "tsx --env-file .env.test team-task-app/simple-test.ts"
  },
  "dependencies": {
    "@vibebase/sdk": "workspace:*",
    "dotenv": "^16.4.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/eventsource": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "dotenv-cli": "^7.3.0",
    "eventsource": "^4.0.0",
    "jsdom": "^26.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}